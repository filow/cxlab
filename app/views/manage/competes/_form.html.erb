<%= form_for(@manage_compete,html:{class:"form-inline"}) do |f| %>
  <%= form_errors(@manage_compete) %>
  <%= f.label :contest %>
  <%= f.select :contest_id,options_for_select(@contest_select_field) %>
  <%= f.label :start_time %>
  <%= f.date_field :start_time,class:"form-control" %>
  <%= f.label :end_time %>
  <%= f.date_field :end_time,class:"form-control" %>

  <%= f.submit %>
<% end %>
